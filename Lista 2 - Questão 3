/*Leia 3 (três) números, verifique e escreva o maior entre os números lidos:*/

const prompt = require('prompt-sync')();

function main(){

    const numero1 = Number(prompt('Número 1: '))
    const numero2 = Number(prompt('Número 2: '))
    const numero3 = Number(prompt('Número 3: '))

    if(numero1 > numero2 && numero1 > numero3){

        console.log(numero1,'é o maior número')

    }else if(numero1 < numero2 && numero2 > numero3){
    
        console.log(numero2,'é o maior numero')

    }else if(numero1 < numero2 && numero2 < numero3){

        console.log(numero3,'é o maior numero')

    }else{

        console.log('Os números são iguais')

    }
   
}

main()
