//Leia 2 números (A, B) e escreva-os em ordem inversa (B, A):
const prompt = require('prompt-sync')();

console.log('Digite dois números inteiros:')
const numero_1 = Number(prompt('Numero 1: '))
const numero_2 = Number(prompt('Numero 2: '))

console.log('A ordem inversa é:',numero_2,',',numero_1,'.')
