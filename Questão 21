//Leia uma temperatura em °F, calcule e escreva a equivalente em °C. t°C = (5 * t°F - 160) / 9:
const prompt = require('prompt-sync')();


console.log('Digite a temperatura em °F:')

const fah = Number(prompt('Temperatura em °F: '))


//Conversão para celcius:

const cel = (( 5 * fah ) - 160 )/9

console.log('A temperatura em celcius é igual a: ',cel,'.')
