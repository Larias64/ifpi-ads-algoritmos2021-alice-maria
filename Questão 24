//Leia um valor em m, calcule e escreva o equivalente em cm.:
const prompt = require('prompt-sync')();


console.log('Digite um valor em metro:')

const m = Number(prompt('Valor em m: '))


//metro para centímetros:

const cm = m * 100

console.log('O valor em metros é igual a:',cm,'em centímetro(s).')
