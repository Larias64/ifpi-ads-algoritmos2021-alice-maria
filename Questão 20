//Leia uma temperatura em °C, calcule e escreva a equivalente em °F. t°F = (9 * t°C + 160) / 5:
const prompt = require('prompt-sync')();


console.log('Digite a temperatura em °C:')

const cel = Number(prompt('Temperatura em °C: '))


//Conversão para fahrenheit

const fah = (( 9 * cel ) + 160 )/5

console.log('A temperatura em fahrenheit é igual a: ',fah,'.')
